{
  "name": "wordpress-block-usm",
  "version": "0.1.0",
  "description": "Gutenberg custom blocks for USM",
  "author": "Marie Rigal",
  "license": "GPL-2.0-or-later",
  "main": "build/index.js",
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "wp-scripts build --webpack-copy-php",
    "format": "wp-scripts format",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip",
    "prestart": "rm -rf build",
    "start": "wp-scripts start --webpack-copy-php",
    "i18n:update": "wp i18n make-pot . languages/usm-block.pot",
    "i18n:json": "wp i18n make-json languages --no-purge --pretty-print"
  },
  "devDependencies": {
    "@wordpress/block-editor": "^11.7.0",
    "@wordpress/blocks": "^12.7.0",
    "@wordpress/data": "^9.0.0",
    "@wordpress/i18n": "^4.30.0",
    "@wordpress/scripts": "^26.1.0"
  },
  "dependencies": {
    "@wordpress/components": "^24.0.0",
    "@wordpress/core-data": "^6.10.0",
    "@wordpress/editor": "^13.10.0",
    "@wordpress/server-side-render": "^4.10.0"
  }
}
