{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "version": "2.0.0",
  "name": "usmb/team-info",
  "icon": "info",
  "title": "Block équipe",
  "description": "Affiche les informations de l'équipe",
  "category": "usm",
  "supports": {
    "html": false
  },
  "attributes": {
    "isEditable": {
      "type": "boolean",
      "default": false
    },
    "isList": {
      "type": "boolean",
      "default": false
    },
    "title": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    },
    "iconStyle": {
      "type": "string",
      "default": "fas"
    },
    "content": {
      "type": "string"
    }
  },
  "variations": [
    {
      "name": "usmb/team-info/custom",
      "icon": "info",
      "title": "Informations",
      "description": "Informations personnalisées",
      "attributes": {
        "icon": "circle-info",
        "isEditable": true
      },
      "example": {},
      "isDefault": true
    },
    {
      "name": "usmb/team-info/referents",
      "icon": "star-filled",
      "title": "Référents",
      "description": "Référents de l'équipe",
      "attributes": {
        "title": "Référents",
        "icon": "star",
        "isList": true
      },
      "example": {}
    },
    {
      "name": "usmb/team-info/trainings",
      "icon": "flag",
      "title": "Entrainements",
      "description": "Entrainements de l'équipe",
      "attributes": {
        "title": "Entrainements",
        "icon": "flag"
      },
      "example": {}
    },
    {
      "name": "usmb/team-info/contacts",
      "icon": "admin-links",
      "title": "Contact",
      "description": "Contacts de l'équipe",
      "attributes": {
        "title": "Contact",
        "icon": "at"
      },
      "example": {}
    }
  ],
  "textdomain": "usmb",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css"
}
